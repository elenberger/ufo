	<Page xmlns="sap.m" id="idPageM" 
	   title="{i18n>blocks}" titleLevel="Auto" showNavButton="false"
		showHeader="true" >
		<content>
			<List xmlns="sap.m" id="idDSO" 
				noDataText="No data" 
				items="{path: '/dsoSet'}">
				<items>

